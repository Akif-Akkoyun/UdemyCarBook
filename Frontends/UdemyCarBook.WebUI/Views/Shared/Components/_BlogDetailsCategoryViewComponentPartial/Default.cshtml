@model List<ResultCategoryDto>

<div class="sidebar-box ftco-animate">
    <div class="categories">
        <h3>Categoriler</h3>
        @foreach (var item in Model)
        {
            <ul>
                <li><a href="#">@item.Name <span>(0)</span></a></li>
            </ul>
        }
    </div>
</div>