<style>
    #map {
        width: 100%;
        height: 500px;
    }
</style>

<script>
    var map = L.map('map').setView([40.9923, 29.0250], 15);

    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
        attribution: '&copy; OpenStreetMap contributors'
    }).addTo(map);

    L.marker([40.9923, 29.0250]).addTo(map)
        .bindPopup("Kadıköy, İstanbul<br>Buradayız!")
        .openPopup();
</script>

